        "SubscriptionId": "#{Arcus.KeyRotation.ServiceBus.SubscriptionId}#",
        "Namespace": "#{Arcus.KeyRotation.ServiceBus.Namespace}#",
        "QueueName": "#{Arcus.KeyRotation.ServiceBus.QueueName}#",
